#include<stdio.h>
int main()
{
 int ch;     //ch for choice
 printf("Choose any number between 1 to 5: ",ch);
 scanf("%d",&ch);
 switch(ch)
 {
 case 1:
 printf("Food item-Burger\n");
 printf("Price-Rs129");
 break;
 case 2:
 printf("Food item-French fries\n");
 printf("Price-99");
 break;
 cae 3:
 printf("Food item-Pizza\n");
 printf("Price-239");
 break;
 case 4:
 printf("Food item-Pasta\n");
 printf("Price-149");
 break;
 case 5:
 printf("Food item-Sandwich\n");
 printf("Price-149");
 break;
 default:
 printf("You entered a wrong choice");
 }
 return 0;
}
